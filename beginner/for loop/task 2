c=0
for i in range(1,11):
    c=(i*10)
    r=input("Are you tired:")
    if r=='y' or r=='yes':
        s=input("do you want to skip the remaining sets(y/n):")
        if s=='y':
            break
    else:
        print(f"{(10-i)*10} jumping jacks are remaining")
if c==100:
    print("Congratulations! You completed the workout")
else:
    print(f"You completed a total of {c} jumping jacks.")
