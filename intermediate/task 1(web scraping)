import requests
from bs4 import BeautifulSoup

url = "https://getwayclassesnotes.netlify.app/2ndyear"
response = requests.get(url)

soup = BeautifulSoup(response.text, "html.parser")
title = soup.title.text
print(f"title of page{title}: ")

paragraphs = soup.find_all("h1")

for p in paragraphs:
    print(f"heading are: {p.text}")

data = soup.find_all("option")
for d in data:
    print(f"other data:{d.text}")
